---
import MainLayout from '../../layouts/MainLayout.astro';
import CoursePage from '../../pages/_CoursePage';
import { getCourseTitles } from '../../data/course-metadata';

const courseTitles = getCourseTitles();

const { courseId } = Astro.params;
const pageTitle = courseId ? courseTitles[courseId] || courseId : 'Course';
---

<MainLayout title={pageTitle}>
	<CoursePage client:load courseId={courseId} />
</MainLayout>
